<strong><legend>Incluir organizacao</legend></strong>
<form class="" action="/postOrganizacao" method="post" enctype="multipart/form-data">
  <div class="form-group">
    <div class="col-md-9">
      <label for="">Nome da organizacao</label>
      <input type="text" name="nome" value="" class="form-control">
    </div>
  </div>

  <div class="form-group">
    <div class="col-md-9">
      <label for="">Pais de origem</label>
      <input type="text" name="pais" value="" class="form-control">
    </div>
  </div>

  <div class="form-group">
    <div class="col-md-9">
      <label for="">Foto da organizacao</label>
      <input type="file" name="foto" value="">
    </div>
  </div>
  <div class="form-group">
    <div class="col-md-9">
      <input type="submit" name="enviar" value="Enviar" class="btn btn-primary">
    </div>
  </div>
</form>
<%if(result.rowCount > 0){%>
  <table>
    <thead>
      <tr>
        <td>Foto</td>
        <td>Nome</td>
        <td>Pa√≠s</td>
      </tr>
    </thead>
    <tbody>
      <%for(var i=0; i < result.rowCount; i++){%>
        <tr>
            <td><img src="http://localhost:5000/<%= result.rows[i].ft_organizacao %>" alt="" width="10%" height="10%"></td>
            <td><%= result.rows[i].nm_organizacao %></td>
            <td><%= result.rows[i].pais_organizacao %></td>
        </tr>
      <%}%>
    </tbody>
  </table>
<%}%>
